{
  "manifest_version": 2,
  "name": "Zenbu Tojiru",
  "version": "0.0.1",
  "description": "Close All Tabs",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "persistent": true,
    "scripts": ["javascript/jquery-1.10.2.min.js", "javascript/bg.js"]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_popup": "popup.html"
  }
}
